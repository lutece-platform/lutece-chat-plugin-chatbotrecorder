<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<plug-in>
    <name>chatbotrecorder</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0</version>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <description>chatbotrecorder.plugin.description</description>
    <provider>chatbotrecorder.plugin.provider</provider>
    <provider-url>https://fr.lutece.paris.fr/</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2019 Mairie de Paris</copyright>
    <db-pool-required>1</db-pool-required>
    <core-version-dependency>
        <min-core-version>7.0.0</min-core-version>
        <max-core-version />
    </core-version-dependency>
    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>CHATBOTRECORDER_MANAGEMENT</feature-id>
            <feature-title>chatbotrecorder.adminFeature.ChatBotRecorderManager.name</feature-title>
            <feature-description>chatbotrecorder.adminFeature.ChatBotRecorderManager.description</feature-description>
            <feature-level>1</feature-level>
            <feature-url>jsp/admin/plugins/chatbotrecorder/SelectChatBot.jsp</feature-url>
            <feature-icon-url></feature-icon-url>
        </admin-feature>
    </admin-features>
    <!-- XPages parameters -->
    <applications></applications>
    <!-- Portlet parameters -->
    <portlets></portlets>
    <!-- Daemons -->
    <daemons></daemons>
</plug-in>